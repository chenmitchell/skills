{
  "name": "nofa-backtest",
  "version": "0.1.0",
  "description": "Crypto trading strategy backtesting and simulated trading API for AI agents. Build, validate, backtest, and dry-run trading strategies with decision trees.",
  "homepage": "https://reclaw.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/nofA-AI/nofa-skills"
  },
  "author": "nofA-AI",
  "license": "MIT",
  "keywords": [
    "ai-agent",
    "trading",
    "backtesting",
    "crypto",
    "strategy",
    "decision-tree"
  ],
  "skill": {
    "entry": "SKILL.md",
    "api_base": "https://api-dev.reclaw.xyz/api/v1",
    "auth": {
      "type": "bearer",
      "prefix": "nofa_",
      "header": "Authorization"
    },
    "endpoints": {
      "register": "POST /agents/register (public, no auth)",
      "me": "GET /agents/me",
      "backtest_run": "POST /backtest/run",
      "backtest_validate": "POST /backtest/validate",
      "dry_run": "POST /trading/dry-run",
      "session_status": "GET /trading/sessions/{id}",
      "session_trades": "GET /trading/sessions/{id}/trades",
      "session_profit": "GET /trading/sessions/{id}/profit",
      "session_stop": "POST /trading/sessions/{id}/stop",
      "x402_backtest": "POST /x402/backtest (XRPL payment required)",
      "x402_dry_run": "POST /x402/dry-run (XRPL payment required)"
    },
    "registration": {
      "public": true,
      "description": "Agents can register directly without prior authentication. A unique API key is generated and must be saved immediately."
    }
  },
  "files": [
    "SKILL.md",
    "references/strategy-generator.md",
    "skill.json"
  ]
}
